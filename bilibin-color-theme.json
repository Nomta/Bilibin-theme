  "tokenColors": [
    {
      "name": "Iron",
      "scope": [
        "support.type.property-name",
        "support.variable",
        "variable"
      ],
      "settings": {
        "foreground": "#fadbb5"
      }
    },
    {
      "name": "Golden Sand",
      "scope": [
        "markup.inline.raw",
        "storage.type",
        "support.constant",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#fadbb5"
      }
    },
    {
      "name": "Sea Green",
      "scope": [
        "constant",
        "keyword.other.unit",
        "variable.language"
      ],
      "settings": {
        "foreground": "#56b6b2"
      }
    },
    {
      "name": "Slate Gray",
      "scope": [
        "punctuation.definition.tag",
        "comment"
      ],
      "settings": {
        "foreground": "#505F63"
      }
    },
   
    {
      "name": "Sand Gray",
      "scope": [
        "meta.brace",
        "punctuation.definition.array",
        "punctuation.definition.binding-pattern",
        "punctuation.definition.block",
        "punctuation.definition.dictionary",
        "punctuation.definition.string",
        "punctuation.section",
        "punctuation.separator",
        "punctuation.support",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#afa57f"
      }
    },
    {
      "name": "Olivine",
      "scope": [
        "string",
        "markup.quote",
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#63C383"
      }
    },
    {
      "name": "Coral",
      "scope": [
        "entity.name.class",
        "entity.name.function",
        "entity.name.type",
        "entity.other.attribute-name",
        "entity.other.inherited-class",
        "markup.heading.setext",
        "support.function"
      ],
      "settings": {
        "foreground": "#ff5e4d"
      }
    },
    {
      "name": "Wine Red",
      "scope": [
        "entity.name.tag",
        "storage"
      ],
      "settings": {
        "foreground": "#cf0818"
      }
    },
    {
      "name": "Steel Blue",
      "scope": [
        "keyword",
        "punctuation.definition.keyword"
      ],
      "settings": {
        "foreground": "#9fafdb"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "markup.bold.markdown",
        "punctuation.definition.bold.markdown"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": [
        "comment",
        "markup.italic.markdown",
        "punctuation.definition.italic.markdown"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
